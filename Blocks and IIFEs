                                                     // ES6
                                                  {
                                                      const a = 1;
                                                      let b = 2;
                                                      var c = 3;
                                                  }

                                                  //console.log(a + b); //não aparecem no console
                                                  console.log(c);     //c consegue ser mostrada por ser function scoped                         







                                    


